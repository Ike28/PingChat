<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="215.0" prefWidth="184.0" styleClass="reqbkg" stylesheets="@../css/explore.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.map222.sfmc.socialnetworkfx.controller.ReceivedReqitemController">
   <children>
      <ImageView fx:id="pfpView" fitHeight="100.0" fitWidth="100.0" layoutX="63.0" layoutY="22.0" pickOnBounds="true" preserveRatio="true" styleClass="pfp">
         <image>
            <Image url="@../images/defaultpfp.png" />
         </image>
      </ImageView>
      <Label fx:id="nameLabel" alignment="CENTER" layoutY="115.0" prefHeight="32.0" prefWidth="225.0" text="Label" textFill="WHITE">
         <font>
            <Font name="Lato Black" size="21.0" />
         </font>
      </Label>
      <Button fx:id="acceptButton" layoutX="14.0" layoutY="153.0" mnemonicParsing="false" onMouseClicked="#acceptClicked" prefHeight="40.0" prefWidth="93.0" styleClass="accbtn" stylesheets="@../css/explore.css" text="ACCEPT" textFill="WHITE">
         <font>
            <Font name="Lato Black" size="14.0" />
         </font>
      </Button>
      <Button fx:id="rejectButton" layoutX="121.0" layoutY="153.0" mnemonicParsing="false" onMouseClicked="#rejectClicked" prefHeight="40.0" prefWidth="93.0" styleClass="rejbtn" stylesheets="@../css/explore.css" text="REJECT" textFill="WHITE">
         <font>
            <Font name="Lato Black" size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
