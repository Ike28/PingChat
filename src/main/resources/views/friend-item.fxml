<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="215.0" prefWidth="225.0" styleClass="bkg" stylesheets="@../css/friend.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.map222.sfmc.socialnetworkfx.controller.FrienditemController">
   <children>
      <ImageView fx:id="pfpView" fitHeight="100.0" fitWidth="100.0" layoutX="63.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" styleClass="pfp" />
      <Label fx:id="nameLabel" alignment="CENTER" layoutY="115.0" prefHeight="32.0" prefWidth="225.0" text="Label" textFill="WHITE">
         <font>
            <Font name="Lato Black" size="21.0" />
         </font>
      </Label>
      <Button fx:id="chatButton" layoutX="71.0" layoutY="161.0" mnemonicParsing="false" onMouseClicked="#openChat" prefHeight="40.0" prefWidth="83.0" styleClass="chatbtn" text="CHAT" textFill="WHITE">
         <font>
            <Font name="Lato Black" size="17.0" />
         </font>
      </Button>
      <Button fx:id="removeButton" layoutX="175.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#promptRemove" prefHeight="32.0" prefWidth="36.0" styleClass="removebtn" text="X" textFill="WHITE">
         <font>
            <Font name="Lato Black" size="16.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
